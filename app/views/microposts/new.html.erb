<%= form_for(@micropost) do |f| %>
	<%= f.text_field :year, :placeholder=>"2018" %>
	<%= f.text_field :month, :placeholder=>"9" %>
	<%= f.text_field :day, :placeholder=>"20" %>
	<%= f.text_field :teama, :placeholder=>"中日" %>
	<%= f.text_field :teamb, :placeholder=>"横浜" %>
	<%= f.text_field :place, :placeholder=>"ナゴヤドーム" %>
	<%= f.text_field :myteam, :placeholder=>"中日" %>
	<%= f.text_field :artist, :placeholder=>"乃木坂46" %>
	<%= f.text_field :livetitle, :placeholder=>"全国ツアー2018" %>
	<%= f.text_field :logo %>
	<%= f.text_field :apoint, :placeholder=>"6" %>
	<%= f.text_field :bpoint, :placeholder=>"3" %>

	<%= f.submit "新規作成" %>

<% end %>
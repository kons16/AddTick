<%= form_for(@micropost) do |f| %>
	<%= f.text_field :year %>
	<%= f.text_field :month %>
	<%= f.text_field :day %>
	<%= f.text_field :teama %>
	<%= f.text_field :teamb %>
	<%= f.text_field :place %>
	<%= f.text_field :myteam %>
	<%= f.text_field :artist %>
	<%= f.text_field :livetitle %>
	<%= f.text_field :logo %>
	<%= f.text_field :apoint %>
	<%= f.text_field :bpoint %>

	<%= f.submit "新規作成" %>

<% end %>